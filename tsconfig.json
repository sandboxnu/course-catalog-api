{
  "compilerOptions": {
    // Don't output the built files - Babel will do that. Tsc just for checking.
    "noEmit": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "lib": ["es2020"],
    "downlevelIteration": true,
    "skipLibCheck": true
  },
  "exclude": ["node_modules"]
}
